{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.php",
            "use": "vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}

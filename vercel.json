{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/home",
            "dest": "/public/home.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
